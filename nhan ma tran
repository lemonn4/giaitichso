import numpy as np

# Định nghĩa hai ma trận
matrix1 = np.array([[0 , -0.06 , 0.02 ],
                    [-0.03 , 0 , 0.05 ],
                    [-0.01 ,0.02 ,0 ]])

matrix2 = np.array([[2 ],
                    [3 ],
                    [5]])

# Nhân hai ma trận
result = np.dot(matrix1, matrix2)+matrix2

# In kết quả
print("Kết quả của phép nhân hai ma trận:")
print(result)
